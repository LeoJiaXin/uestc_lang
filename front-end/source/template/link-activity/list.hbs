<div class="table">
  <div class="table-row table-header">
    <div class="table-column-3">name</div>
    <div class="table-column-3">desc</div>
    <div class="clear"></div>
  </div>
  {{#each list}}
  <div class="table-row">
    <div class="table-column-3"><a href="#content/{{id}}">{{name}}</a></div>
    <div class="table-column-3">{{desc}}</div>
    <div class="clear"></div>
  </div>
  {{/each}}
</div>
<div id="list-pager"></div>